{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@payload-config": [
        "./src/payload.config.ts"
      ]
    },
    "target": "ES2022",
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/app/domains/domains.js", "src/app/(custom)/domains/auction/bid/route.js", "src/app/(custom)/domains/auction/route.js", "src/app/(custom)/domains/sale/route.js", "src/app/(custom)/domains/sell/route.js", "src/app/(custom)/domains/views/route.js", "src/app/(custom)/domains/route.js", "src/app/(custom)/leads/route.js", "src/app/(custom)/leads/create/route.js", "src/app/(custom)/leads/message/route.js", "src/app/(custom)/sales/route.js", "src/routes/leads/route.js", "src/app/(custom)/domains/offer/route.js"  ],
  "exclude": [
    "node_modules"
  ],
}
